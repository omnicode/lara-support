{
	"name": "omnicode/lara-support",
	"description": "Additions for laravel support",
	"keywords": ["laravel", "support"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.5.*",
		"omnicode/php-util": "3.0.*"
	},
	"require-dev": {
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~6.0",
		"omnicode/lara-test": "~0.0",
		"orchestra/testbench": "~3.0"

	},
	"autoload": {
		"psr-4": {
			"LaraSupport\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
