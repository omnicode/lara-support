{
	"name": "omnicode/lara-support",
	"description": "Additions for laravel support",
	"keywords": ["laravel", "support"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.4.*",
		"omnicode/php-util": "0.1.*"
	},
	"require-dev": {
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~6.0",
		"orchestra/testbench": "~3.0"

	},
	"autoload": {
		"psr-4": {
			"LaraSupport\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"LaraSupport\\ServiceProvider\\LaraSupportServiceProvider"
			],
			"aliases": {
				"LaraDB": "LaraSupport\\Facades\\LaraDB",
				"LaraPassword": "LaraSupport\\Facades\\LaraPassword"
			}
		}
	}
}
