{
	"name": "omnicode/lara-support",
	"description": "Additions for laravel support",
	"keywords": ["laravel", "support"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.6.*",
		"omnicode/php-util": "3.0.*"
	},
	"require-dev": {
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~7.0",
		"omnicode/lara-test": "~3.0",
		"orchestra/testbench": "~3.0"

	},
	"autoload": {
		"psr-4": {
			"LaraSupport\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
        "laravel": {
            "providers": [
                " LaraSupport\\ServiceProvider\\LaraSupportServiceProvider "
            ]
        }
    }
}
