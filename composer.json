{
	"name": "omnicode/lara-support",
	"description": "Additions for laravel support",
	"keywords": ["laravel", "support"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.3.* || 5.4.* || 5.5.*"
	},
	"require-dev": {
		"mockery/mockery": "~0.9",
		"phpunit/phpunit": "~4.0|~5.0"
	},
	"autoload": {
		"psr-4": {
			"LaraSupport\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
